	Journal = {Acta Neuropathol},
	Journal-Full = {Acta neuropathologica},
	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Apolipoprotein E4; Atrophy; Brain; Cognition; DNA-Binding Proteins; Female; Humans; Immunohistochemistry; Magnetic Resonance Imaging; Male; Memory Disorders; Middle Aged; Regression Analysis; Sclerosis; Severity of Illness Index},
	Number = {6},
	Pages = {811-24},
	Pmc = {PMC4172544},
