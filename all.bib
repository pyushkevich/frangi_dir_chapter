	Pages = {179-190},
	Pii = {S0959-4388(06)00038-9},
	Pmid = {16564688},
	Timestamp = {2006.04.21},
	Title = {{The cognitive neuroscience of remote episodic, semantic and spatial memory.}},
	Url = {http://dx.doi.org/10.1016/j.conb.2006.03.013},
	Volume = {16},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.conb.2006.03.013}}

