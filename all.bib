	Mesh = {Algorithms; Artificial Intelligence; Image Enhancement; Image Interpretation, Computer-Assisted; Information Storage and Retrieval; Models, Statistical; Pattern Recognition, Automated; Reproducibility of Results; Sensitivity and Specificity},
	Month = {Nov},
