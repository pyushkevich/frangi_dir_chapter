	Month = {Oct},
	Number = {7},
	Pages = {693-702},
	Pmc = {PMC3233762},
	Pmid = {14512209},
	Pst = {ppublish},
	Title = {Long-term treatment with paroxetine increases verbal declarative memory and hippocampal volume in posttraumatic stress disorder},
	Volume = {54},
