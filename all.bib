	Mesh = {Algorithms; Brain; Databases, Factual; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Models, Statistical; Multiple Sclerosis},
	Month = {May},
	Number = {5},
	Pages = {1281-92},
