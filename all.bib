	Doi = {10.1016/j.celrep.2019.08.008},
	Journal = {Cell Rep},
	Journal-Full = {Cell reports},
	Keywords = {brain network; brain stimulation; network control theory; ASHS},
	Month = {Sep},
	Number = {10},
