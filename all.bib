	Doi = {10.1161/CIRCULATIONAHA.106.649236},
	Journal = {Circulation},
	Journal-Full = {Circulation},
	Mesh = {Aged; Echocardiography; Female; Humans; Male; Middle Aged; Mitral Valve; Mitral Valve Insufficiency; Myocardial Ischemia; Reproducibility of Results; Ventricular Remodeling},
	Month = {Feb},
	Number = {6},
	Pages = {782-91},
	Pmid = {17283262},
	Pst = {ppublish},
	Title = {Preoperative posterior leaflet angle accurately predicts outcome after restrictive mitral valve annuloplasty for ischemic mitral regurgitation},
