	Keywords = {Bayesian modeling; Hippocampal subfields; Monte Carlo sampling; Segmentation},
	Month = {Oct},
	Number = {7},
	Pages = {766-78},
	Pmc = {PMC3719857},
	Pmid = {23773521},
	Pst = {ppublish},
	Title = {Improved inference in Bayesian segmentation using Monte Carlo sampling: application to hippocampal subfield volumetry},
	Volume = {17},
	Year = {2013},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.media.2013.04.005}}

@article{shi12,
	Abstract = {Non-rigid image registration using free-form deformations (FFD) is a widely used technique in medical image registration. The balance between robustness and accuracy is controlled by the control point grid spacing and the amount of regularization. In this paper, we revisit the classic FFD registration approach and propose a sparse representation for FFDs using the principles of compressed sensing. The sparse free-form deformation model (SFFD) can capture fine local details such as motion discontinuities without sacrificing robustness. We demonstrate the capabilities of the proposed framework to accurately estimate smooth as well as discontinuous deformations in 2D and 3D image sequences. Compared to the classic FFD approach, a significant increase in registration accuracy can be observed in natural images (61%) as well as in cardiac MR images (53%) with discontinuous motions.},
	Author = {Shi, Wenzhe and Zhuang, Xiahai and Pizarro, Luis and Bai, Wenjia and Wang, Haiyan and Tung, Kai-Pin and Edwards, Philip and Rueckert, Daniel},
