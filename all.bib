	Mesh = {Adult; Algorithms; Cluster Analysis; Female; Fourier Analysis; Humans; Linear Models; Magnetic Resonance Imaging; Male; Neural Pathways; Normal Distribution; Oxygen; Rest; Software; Young Adult},
