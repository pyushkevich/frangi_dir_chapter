	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Atrophy; Dementia, Vascular; Diagnosis, Differential; Female; Hippocampus; Humans; Lewy Body Disease; Longitudinal Studies; Magnetic Resonance Imaging; Male; Prospective Studies; Temporal Lobe},
	Month = {Jan},
