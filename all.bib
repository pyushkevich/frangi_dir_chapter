	Mesh = {Adolescent; Adult; Case-Control Studies; Female; Gene Frequency; Genetic Association Studies; Genotype; Hippocampus; Humans; Image Processing, Computer-Assisted; Individuality; Intracellular Signaling Peptides and Proteins; Magnetic Resonance Imaging; Male; Phosphoproteins; Polymorphism, Genetic; Reproducibility of Results; Young Adult},
	Month = {Aug},
	Number = {32},
	Pages = {13088-93},
	Pmid = {23926262},
