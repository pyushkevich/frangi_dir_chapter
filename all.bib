	Keywords = {Aged, Algorithms, Alzheimer Disease, Atrophy, Cognition Disorders, Computer-Assisted, Extramural, Female, Hippocampus, Humans, Image Processing, Laterality, Male, Models, N.I.H., P.H.S., Research Support, Software, Statistical, U.S. Gov't, 15990339},
	Month = {Oct},
	Number = {4},
	Pages = {979-90},
	Pii = {S1053-8119(05)00316-2},
	Title = {Atlas-based hippocampus segmentation in {A}lzheimer's disease and mild cognitive impairment.},
	Url = {http://dx.doi.org/10.1016/j.neuroimage.2005.05.005},
	Volume = {27},
	Year = {2005},
