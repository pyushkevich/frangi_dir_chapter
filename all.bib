	Pii = {S0304394097005600},
	Pmid = {9300651},
	Timestamp = {2010.06.11},
	Title = {Diet induced hyperammonemia decreases neuronal nuclear size in rat entorhinal cortex.},
	Volume = {231},
	Year = {1997}}

@article{insausti98a,
	Abstract = {Ts65Dn mouse displays a partial triplication of chromosome 16 and
	is adopted as a model for Down syndrome (DS). It is known that Ts65Dn
	mice present memory deficiencies. In order to gain insight into the
	cause of these deficiencies, we studied the possibility of changes
	in volumes and neuronal numbers in different regions of the hippocampus
	(dentate gyrus, CA3, CA2 and CA1) in trisomic mice as compared to
	control littermates using stereological methods. The mean hippocampal
	volumes of Ts65Dn mice did not show significant differences as compared
	to controls, except in CA2 where there was a barely significant decrease.
	However, mean neuron number was significantly lower in Ts65Dn mice
	than in controls in dentate gyrus (43.7 x 10(4), CV 21\%, n = 5,
	vs. 30.4 x 10(4), CV 18.1\%, n = 4) and higher in CA3 (23.1 x 10(4),
	CV 18.9\% vs. 33.3 x 10(4), CV 14.9\%). These quantitative changes
	may account for the memory deficiencies observed in Ts65Dn mice.},
	Institution = {Department of Physiology and Pharmacology, Faculty of Medicine, University of Cantabria, Santander, Spain.},
	Journal = {Neurosci Lett},
	Keywords = {Animals; Cell Count; Dentate Gyrus, abnormalities; Disease Models, Animal; Down Syndrome, genetics; Hippocampus, anatomy /&/ histology/pathology; Immunohistochemistry; Mice; Mice, Neurologic Mutants; Trisomy, genetics},
	Month = {Sep},
	Number = {3},
	Owner = {pauly},
	Pages = {175-178},
	Pii = {S0304394098006417},
	Pmid = {9792239},
	Timestamp = {2010.06.11},
	Title = {Hippocampal volume and neuronal number in Ts65Dn mice: a murine model of Down syndrome.},
	Volume = {253},
	Year = {1998}}

@article{insausti02,
	Abstract = {Our objective was to evaluate our experience with the Advanced Breast
	Biopsy Instrumentation system (ABBI) in non-palpable breast lesions
	in a prospective study from July 1998 to November 2000. The ABBI
	system was included in a protocol for BIRADS 4 non-palpable, small
	(<15 mm) breast lesions. Digital radiographs of both specimen and
	biopsy cavity were obtained to validate the procedure. A total of
	255 ABBI biopsies were performed in 254 patients. In 251 cases the
	lesions were successfully removed (98.4\%). Mammographic lesions
	consisted of 176 cases of microcalcifications (69\%), 51 cases of
	architectural distortions (20\%) and 28 cases of nodules (11\%).
	Seventy-two carcinomas were diagnosed (28.2\%). Affected margins
	were found in 41 cases (56.9\%). Residual tumour was seen in 31 patients
	(43\%). Seventeen borderline results and 33 benign architectural
	distortions obviated further procedures. The complication rate in
	10 cases was as follows: 3 wound infections; 4 haematomas; and 3
	vasovagal reactions. The main utility of the ABBI system is to allow
	a reliable diagnosis in complex lesions, such as small clusters of
	microcalcifications and especially architectural distortions. Surgery
	can be avoided for borderline cases if the lesion is completely removed
	and free margins are obtained in the pathology study. Therapeutic
	use is controversial and can be applied only in selected cases.},
	Doi = {10.1007/s00330-001-1250-9},
	Institution = {Department of Radiology, Clinica Universitaria de Navarra, C/Pio XII, 36, 31008 Pamplona, Spain. ljpina@unav.es},
	Journal = {Eur Radiol},
	Keywords = {Adult; Aged; Biopsy, instrumentation/methods; Breast Neoplasms, diagnosis/radiography/surgery; Breast, pathology; Female; Humans; Mammography; Middle Aged; Palpation; Prospective Studies; Radiography, Interventional},
	Month = {Jul},
	Number = {7},
	Owner = {pauly},
	Pages = {1703-1710},
	Pmid = {12111061},
	Timestamp = {2010.06.11},
	Title = {An experience with the Advanced Breast Biopsy Instrumentation (ABBI) system in the management of non-palpable breast lesions.},
	Url = {http://dx.doi.org/10.1007/s00330-001-1250-9},
	Volume = {12},
	Year = {2002},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s00330-001-1250-9}}

@article{insausti98b,
	Abstract = {PURPOSE: Our purpose was to investigate the normal volumes of the
	human entorhinal, perirhinal, and temporopolar cortices on MR imaging
	studies using a customized program. METHODS: We designed a protocol
	in which the volumes of the entorhinal, perirhinal, and temporopolar
	cortices were determined from coronal MR images using anatomic landmarks
	defined on the basis of cytoarchitectonic analyses of 49 autopsy
	cases. MR volumetry of these cortical areas was performed in 52 healthy
	volunteers. RESULTS: The overall mean volumes were 1768 +/- 328 mm3/1558
	+/- 341 mm3 (right/left) for the entorhinal cortex, 2512 +/- 672
	mm3/2572 +/- 666 mm3 for the perirhinal cortex, and 2960 +/- 623
	mm3/3091 +/- 636 mm3 for the temporopolar cortex. The right entorhinal
	cortex was 12\% larger than the left. The volume of the temporopolar
	cortex was reduced bilaterally by 13\% in the older age group compared
	with younger subjects, while the volumes of the entorhinal and perirhinal
	cortices were unaffected by age. There were no differences between
	men and women in the volumes of any of the three cortices. CONCLUSION:
	Our method provides a tool by which to measure volumes of the entorhinal,
	perirhinal, and temporopolar cortices on coronal MR images.},
	Author = {R. Insausti and K. Juottonen and H. Soininen and A. M. Insausti and K. Partanen and P. Vainio and M. P. Laakso and A. Pitk\"{a}nen},
