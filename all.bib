	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Brain; Cognition Disorders; Disease Progression; Female; Humans; Longitudinal Studies; Magnetic Resonance Imaging; Male; Neuropsychological Tests; Positron-Emission Tomography; Severity of Illness Index},
	Number = {3},
	Pages = {e33182},
	Pmc = {PMC3310854},
	Pmid = {22457741},
	Pst = {ppublish},
	Title = {Predicting future clinical changes of MCI patients using longitudinal and multimodal biomarkers},
	Volume = {7},
