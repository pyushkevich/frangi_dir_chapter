@article{dickson09,
	Abstract = {Neurodegenerative diseases are characterized by selective and progressive loss of specific populations of neurons, which determines the clinical presentation. The same neuronal populations can be affected in a number of different disorders. Given that the clinical presentation reflects the particular population of neurons that are targets of the disease process, it is clear that for any given clinical syndrome, more than one neurodegenerative disease can account for the clinical syndrome. Because of this clinical ambiguity, for the purpose of this brief review neurodegenerative disorders are classified according to the underlying molecular pathology rather than their clinical presentation. The major neurodegenerative diseases can be classified into amyloidoses, tauopathies, alpha-synucleinopathies and TDP-43 proteinopathies.},
	Author = {Dickson, Dennis W},
