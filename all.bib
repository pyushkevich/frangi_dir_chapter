	Pages = {52-60},
	Pii = {S1053-8119(05)00716-0},
	Pmid = {16243546},
	Timestamp = {2007.12.12},
	Title = {{Abnormalities of hippocampal surface structure in very mild dementia of the Alzheimer type.}},
	Url = {http://dx.doi.org/10.1016/j.neuroimage.2005.09.017},
	Volume = {30},
	Year = {2006},
