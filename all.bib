	Keywords = {Algorithms, Cardiovascular, Computer Simulation, Computer-Assisted, Diffusion Magnetic Resonance Imaging, Heart, Image Enhancement, Image Interpretation, Imaging, Models, Reproducibility of Results, Sensitivity and Specificity, Three-Dimensional, 16156359},
