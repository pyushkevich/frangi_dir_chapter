	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Cell Count; Cognition Disorders; Entorhinal Cortex; Hippocampus; Humans; Middle Aged; Neurofibrillary Tangles; Neurons},
	Month = {Sep},
	Number = {9},
