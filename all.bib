	Journal = {Arch Neurol},
	Journal-Full = {Archives of neurology},
	Mesh = {Adult; Central Nervous System; Cohort Studies; Contrast Media; Disease Progression; Early Diagnosis; Female; Follow-Up Studies; Gadolinium; Humans; Magnetic Resonance Imaging; Male; Multiple Sclerosis; Predictive Value of Tests; Prognosis; Reproducibility of Results; Time Factors},
