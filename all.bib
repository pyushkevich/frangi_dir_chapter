	Journal-Full = {Cerebral cortex (New York, N.Y. : 1991)},
	Mesh = {Age Distribution; Aged; Aged, 80 and over; Aging; Brain; Cell Count; Female; Hippocampus; Humans; Male; Middle Aged; Multivariate Analysis; Neurons; Sex Distribution},
	Month = {Dec},
	Number = {8},
