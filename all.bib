	Keywords = {Alzheimer's disease; DTI; hippocampus; mean diffusivity; subfield; volume},
	Mesh = {Aged; Aged, 80 and over; Aging; Alzheimer Disease; Atrophy; CA1 Region, Hippocampal; Cognition Disorders; Diffusion Tensor Imaging; Female; Hippocampus; Humans; Male; Middle Aged; ROC Curve; Severity of Illness Index},
	Month = {Sep},
