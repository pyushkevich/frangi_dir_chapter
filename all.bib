	Keywords = {CA3; DG; MVPA; fMRI; hippocampus; memory; scenes; subfields},
	Pages = {290},
	Pmc = {PMC3474998},
	Pmid = {23087638},
	Pst = {epublish},
	Title = {Multi-voxel pattern analysis in human hippocampal subfields},
	Volume = {6},
