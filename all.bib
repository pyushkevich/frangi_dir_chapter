	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Biomarkers; Cognition Disorders; Demography; Female; Humans; Linear Models; Magnetic Resonance Imaging; Male; Middle Aged; Organ Specificity; Sample Size},
	Month = {Dec},
	Number = {49},
	Pages = {20954-9},
