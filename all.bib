	Pages = {1731-40},
	Pii = {awh197},
	Title = {M{RI}-based high-dimensional hippocampal mapping in mesial temporal lobe epilepsy.},
	Url = {http://dx.doi.org/10.1093/brain/awh197},
	Volume = {127},
	Year = {2004},
