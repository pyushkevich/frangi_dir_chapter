	Mesh = {Adult; Aged; Amygdala; Control Groups; Depressive Disorder, Major; Female; Functional Laterality; Hippocampus; Humans; Image Processing, Computer-Assisted; Imaging, Three-Dimensional; Magnetic Resonance Imaging; Male; Parkinson Disease; Reproducibility of Results},
	Month = {Jul},
