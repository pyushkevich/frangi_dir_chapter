	Doi = {10.1002/gps.3774},
	Journal = {Int J Geriatr Psychiatry},
	Journal-Full = {International journal of geriatric psychiatry},
	Mesh = {Aged; Aged, 80 and over; Atrophy; Autopsy; Dementia; Female; Hippocampus; Humans; Magnetic Resonance Imaging; Male; Regression Analysis},
	Month = {Dec},
	Number = {12},
	Pages = {1267-74},
	Pmid = {22383167},
