	Journal = {Neuroimage},
	Keywords = {Algorithms; Cadaver; Computer Simulation; Hippocampus, anatomy /&/ histology; Humans; Image Enhancement, methods; Image Interpretation, Computer-Assisted, methods; Imaging, Three-Dimensional, methods; Magnetic Resonance Imaging, methods; Models, Anatomic; Reproducibility of Results; Sensitivity and Specificity},
