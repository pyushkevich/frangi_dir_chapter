	Journal = {Neurobiol Aging},
	Journal-Full = {Neurobiology of aging},
	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Cognitive Dysfunction; Disease Progression; Early Diagnosis; Female; Humans; Male; Nerve Degeneration; Neurofibrillary Tangles; Plaque, Amyloid},
