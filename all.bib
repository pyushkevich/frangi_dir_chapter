	Pages = {S99-110},
	Pii = {S1053-8119(08)01174-9},
	Pmid = {19059348},
	Timestamp = {2009.03.06},
	Title = {{Continuous medial representation of brain structures using the biharmonic PDE.}},
	Volume = {45},
