	Owner = {Paul},
	Pages = {1139-45},
	Pii = {nn739},
	Title = {Consolidation of human memory over decades revealed by functional magnetic resonance imaging.},
	Url = {http://dx.doi.org/10.1038/nn739},
	Volume = {4},
	Year = {2001},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nn739}}

