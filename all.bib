	Doi = {10.1016/j.neurobiolaging.2010.05.023},
	Journal = {Neurobiol Aging},
	Journal-Full = {Neurobiology of aging},
	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Biological Markers; Disease Progression; Female; Follow-Up Studies; Humans; Magnetic Resonance Imaging; Male; Mild Cognitive Impairment; Neuropsychological Tests; Predictive Value of Tests},
	Month = {Dec},
