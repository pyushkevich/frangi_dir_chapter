	Doi = {10.1002/ana.21557},
	Journal = {Ann Neurol},
	Journal-Full = {Annals of neurology},
	Mesh = {Aged; Aged, 80 and over; Aging; Animals; Blood Glucose; Cognition Disorders; Diabetes Mellitus, Type 2; Female; Hippocampus; Humans; Macaca mulatta; Male; Mice; Mice, Inbred C57BL; Neuropsychological Tests},
	Month = {Dec},
