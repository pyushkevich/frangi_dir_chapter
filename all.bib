	Keywords = {APOE; TDP-43; hippocampus; human; oldest-old},
	Mesh = {Age Factors; Aged; Aged, 80 and over; Aging; Alzheimer Disease; Autopsy; Female; Frontotemporal Lobar Degeneration; Hippocampus; Humans; Male; Neurofibrillary Tangles; Psychiatric Status Rating Scales; Sclerosis; Statistics as Topic; tau Proteins},
