	Journal = {Epilepsia},
	Journal-Full = {Epilepsia},
	Mesh = {Adolescent; Adult; Age of Onset; Atrophy; Biological Markers; Cerebral Cortex; Chronic Disease; Cross-Sectional Studies; Diagnosis, Computer-Assisted; Epilepsy, Temporal Lobe; Female; Functional Laterality; Hippocampus; Humans; Magnetic Resonance Imaging; Male; Middle Aged; Recurrence; Severity of Illness Index; Temporal Lobe},
	Month = {Mar},
