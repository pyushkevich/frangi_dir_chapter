	Timestamp = {2007.12.12},
	Title = {{High-resolution fMRI investigation of the medial temporal lobe.}},
	Url = {http://dx.doi.org/10.1002/hbm.20331},
	Volume = {28},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1002/hbm.20331}}

