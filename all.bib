	Institution = {Department of Neurology, University of Pennsylvania School of Medicine, Philadelphia, PA 19104-4283, USA. mgrossman@mail.med.upennn.edu},
	Journal = {Ann Neurol},
	Keywords = {Onset; Aged; Alzheimer Disease, cerebrospinal fluid/psychology; Amyloid beta-Protein, cerebrospinal fluid; Atrophy; Biological Markers; Cerebral Cortex, pathology; Cohort Studies; Dementia, cerebrospinal fluid/psychology; Female; Frontal Lobe; Humans; Isoprostanes, cerebrospinal fluid; Magnetic Resonance Imaging; Male; Middle Aged; Neuropsychological Tests; Phenotype; Prospective Studies; ROC Curve; tau Proteins, cerebrospinal fluid},
