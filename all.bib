	Mesh = {Aged; Aged, 80 and over; Alzheimer Disease; Atrophy; Female; Hippocampus; Humans; Magnetic Resonance Imaging; Male; Memory Disorders; Mental Recall; Middle Aged; Neuropil},
	Month = {Oct},
	Number = {1},
	Pages = {194-202},
