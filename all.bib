	Owner = {pauly},
	Pages = {4087-4095},
	Pmid = {18841861},
	Timestamp = {2011.02.02},
	Title = {Computer-aided detection of masses in digital tomosynthesis mammography: comparison of three approaches.},
	Volume = {35},
