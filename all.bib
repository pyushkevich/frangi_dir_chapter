	Doi = {10.1016/j.jtcvs.2008.03.027},
	Journal = {J Thorac Cardiovasc Surg},
	Journal-Full = {The Journal of thoracic and cardiovascular surgery},
	Mesh = {Aged; Blood Flow Velocity; Coronary Artery Bypass; Female; Humans; Male; Middle Aged; Mitral Valve; Mitral Valve Insufficiency; Papillary Muscles; Recurrence; Stroke Volume; Ventricular Dysfunction, Left; Ventricular Remodeling},
