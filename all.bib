	Keywords = {Alzheimer's disease; aging; atrophy; cortical thickness; magnetic resonance imaging},
	Mesh = {Adolescent; Adult; Age Factors; Aged; Aged, 80 and over; Aging; Alzheimer Disease; Amyloid beta-Peptides; Cerebral Cortex; Cross-Sectional Studies; Dementia; Disease Progression; Female; Humans; Imaging, Three-Dimensional; Longitudinal Studies; Male; Middle Aged; Neuropsychological Tests; Peptide Fragments; Young Adult},
