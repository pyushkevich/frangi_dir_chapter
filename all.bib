	Mesh = {Aged; Aged, 80 and over; Aging; Alzheimer Disease; Artificial Intelligence; Brain; Cohort Studies; Female; Humans; Linear Models; Magnetic Resonance Imaging; Male; Middle Aged; Mild Cognitive Impairment; Risk; Signal Processing, Computer-Assisted; Support Vector Machines; White Matter},
	Month = {Aug},
