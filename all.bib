	Title = {[The entorhinal cortex (hippocampal formation) in aging and Alzheimer's disease. Neuroanatomical interpretation].},
	Volume = {41},
	Year = {1997}}

@article{sole01,
	Abstract = {A novel method for denoising functional magnetic resonance imaging
	temporal signals is presented in this note. The method is based on
