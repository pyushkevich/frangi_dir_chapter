	Journal = {Radiology},
	Keywords = {Adult; Analysis of Variance; Anisotropy; Body Water; Brain; Diffusion; Female; Follow-Up Studies; Gestational Age; Humans; Infant, Newborn; Least-Squares Analysis; Magnetic Resonance Imaging; Male; Reference Values},
	Month = {Oct},
